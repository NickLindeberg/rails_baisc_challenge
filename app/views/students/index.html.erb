<h1>All Students</h1>

<%= link_to 'Create New Student', new_student_path %>
<ul>
  <% @students.each do |student|  %>
    <li>
      <%= student.name, student_path(student) %>
      <%= link_to "Edit", edit_student_path(student) %>
      <%= link_to "Delete", student_path(student), method: "delete" %>
    </li>
  <% end %>
</ul>
